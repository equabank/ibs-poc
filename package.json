{
  "name": "equa",
  "private": true,
  "engines": {
    "node": ">= 8.0.0"
  },
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "start": "cross-env NODE_ENV=production node server.js",
    "deploy": "node ./scripts/deploy",
    "extract-messages": "npm run clean-babel-cache && rimraf lang/.messages && next build && node ./scripts/extractMessages",
    "favicon": "node ./scripts/favicon",
    "flow": "flow",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "prettier": "prettier --single-quote --trailing-comma all --write '{components,lib,mutations,pages,reducers,scripts,test,themes,types}/**/*.js'",
    "clean-babel-cache": "rimraf node_modules/.cache/babel-loader",
    "test": "flow && jest --updateSnapshot && npm run relay && npm run build",
    "test-without-relay": "flow && jest --updateSnapshot && npm run build",
    "deps": "rimraf node_modules package-lock.json flow-typed && npm install && flow-typed install",
    "deps-clean": "rimraf package-lock.json flow-typed node_modules",
    "deps-refresh": "npm install && flow-typed install",
    "analyze": "cross-env ANALYZE=1 next build",
    "relay": "rimraf **/__generated__ && relay-compiler --src ./ --exclude **/.next/** **/flow-typed/** **/node_modules/** **/test/**  **/__generated__/** --schema data/schema.graphql",
    "relay:watch": "npm run relay -- --watch",
    "schema": "graphql get-schema dev"
  },
  "dependencies": {
    "accepts": "^1.3.3",
    "babel-plugin-react-intl": "^2.3.1",
    "color": "^2.0.0",
    "cookie": "^0.3.1",
    "cross-env": "^5.0.1",
    "fela": "^5.0.1",
    "fela-dom": "^5.0.2",
    "fela-plugin-placeholder-prefixer": "^5.0.1",
    "fela-preset-web": "^5.0.2",
    "glob": "^7.1.2",
    "gravatar": "^1.6.0",
    "intl": "^1.2.5",
    "isomorphic-fetch": "^2.2.1",
    "next": "^3.0.3",
    "nprogress": "^0.2.0",
    "prop-types": "^15.6.0",
    "raf": "^3.3.2",
    "ramda": "^0.24.1",
    "raven": "^2.1.1",
    "raven-js": "^3.17.0",
    "react": "^15.6.0",
    "react-dom": "^15.6.0",
    "react-fela": "^5.0.3",
    "react-intl": "^2.3.0",
    "react-redux": "^5.0.3",
    "react-relay": "^1.2.0",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "relay-runtime": "^1.2.0",
    "rimraf": "^2.6.2",
    "rxjs": "^5.4.0",
    "smoothscroll-polyfill": "^0.4.0",
    "url-parse": "^1.1.9",
    "url-search-params": "^0.10.0",
    "validator": "^9.0.0",
    "webpack-bundle-analyzer": "^2.8.2"
  },
  "devDependencies": {
    "babel-jest": "^20.0.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-relay": "^1.2.0",
    "babel-plugin-transform-define": "^1.2.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-loader": "^1.7.0",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.0.1",
    "flow-bin": "^0.56.0",
    "flow-typed": "^2.1.5",
    "graphql-cli": "^1.0.1",
    "jest": "^20.0.0",
    "prettier": "^1.5.1",
    "react-test-renderer": "^16.0.0",
    "relay-compiler": "^1.2.0"
  }
}
